{
  "name": "yslt",
  "version": "1.1.0",
  "description": "You should listen to",
  "main": "index.js",
  "devDependencies": {
    "browser-sync": "^2.9.1",
    "csso": "^1.3.11",
    "envify": "^3.4.0",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^3.3.1",
    "html-inline": "^1.1.1",
    "js-beautify": "^1.5.10",
    "jspm": "^0.16.2",
    "phantomjs": "^1.9.18",
    "rimraf": "^2.4.3",
    "surge": "^0.14.3",
    "uglify-js": "^2.4.24",
    "watch": "^0.16.0"
  },
  "scripts": {
    "update": "phantomjs phantom/metagrabber.js",
    "clean": "rimraf dist/ && mkdir dist/",
    "jsLint": "eslint lib/",
    "jsBundle": "jspm bundle-sfx lib/main build/app.js",
    "jsMinify": "export NODE_ENV=production && envify build/app.js | uglifyjs --mangle -o build/app.min.js",
    "buildCss": "csso -i css/style.css -o css/style.min.css",
    "buildJs": "npm run jsLint && npm run jsBundle && npm run jsMinify",
    "build": "npm run buildJs && npm run buildCss",
    "distCopy": "cp -r CNAME dist/ && cp -r build/ dist/ && cp -r data/ dist/",
    "distCritical": "html-inline --ignore-images --ignore-scripts -i index.html -o dist/index.html",
    "dist": "npm run clean && npm run build && npm run distCopy && npm run distCritical",
    "start": "npm run build && browser-sync start --server",
    "test": "npm run jsLint",
    "watch": "npm run build && watch scripts/build lib/",
    "deploy": "npm run dist && surge dist/"
  },
  "author": "David Rapson",
  "repository": "https://github.com/davidrapson/yslt.git",
  "license": "ISC",
  "jspm": {
    "dependencies": {
      "classnames": "npm:classnames@^1.1.4",
      "jsonp": "npm:jsonp@^0.1.0",
      "react": "npm:react@^0.14.0-alpha1",
      "react-dom": "npm:react-dom@^0.14.0-beta2",
      "whatwg-fetch": "npm:whatwg-fetch@^0.7.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  }
}
