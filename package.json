{
  "name": "yslt",
  "version": "1.0.0",
  "description": "You should listen to",
  "main": "index.js",
  "devDependencies": {
    "browser-sync": "^2.6.4",
    "csso": "^1.3.11",
    "eslint": "^0.20.0",
    "js-beautify": "^1.5.5",
    "phantomjs": "^1.9.16",
    "watch": "^0.16.0"
  },
  "scripts": {
    "lint": "eslint lib",
    "jspm": "jspm bundle-sfx --minify lib/main.jsx! build/app.js",
    "build-css": "csso -i css/style.css -o css/style.min.css",
    "build-js": "npm run lint && npm run jspm",
    "build": "npm run build-js && npm run build-css",
    "watch": "npm run build && watch scripts/build lib/",
    "start": "npm run build && browser-sync start --server",
    "update": "phantomjs phantom/metagrabber.js",
    "deploy": "npm run build && surge"
  },
  "author": "David Rapson",
  "repository": "https://github.com/davidrapson/yslt.git",
  "license": "ISC",
  "jspm": {
    "dependencies": {
      "classnames": "npm:classnames@^1.1.4",
      "jsonp": "npm:jsonp@^0.1.0",
      "jsx": "github:floatdrop/plugin-jsx@^1.1.0",
      "react": "npm:react@^0.13.0",
      "whatwg-fetch": "npm:whatwg-fetch@^0.7.0"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.87",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.87"
    }
  }
}
